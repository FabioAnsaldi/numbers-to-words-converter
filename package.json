    "babel-plugin-import-glob": "^2.0.0",
    "connect-history-api-fallback": "^1.6.0",
    "express": "^4.16.4",
    "hoist-non-react-statics": "^3.3.0",
    "invariant": "^2.2.4",
    "json-server": "^0.14.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
